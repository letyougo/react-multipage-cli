{
  "presets": [
    ["env", { "modules": false }],
    "stage-2",
    "react"
  ],
  "plugins": [
    ["transform-runtime"],
    ["import",[{
      "libraryName": "antd",
      "libraryDirectory": "lib",
      "style": true
    }]]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul"]
    },
    "production": {
      "presets": ["react-optimize"]
    }
  }
}
